<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomButtonState">
        <!-- Use one for every state you want to have  -->
        <attr name="state_one" format="boolean" />
        <attr name="state_two" format="boolean" />
        <attr name="state_three" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="collapsedHeight" format="dimension" />
        <attr name="shadowHeight" format="dimension" />
        <attr name="fadeColor" format="color" />
        <attr name="flingVelocity" format="integer" />
        <attr name="dragView" format="reference" />
    </declare-styleable>
</resources>